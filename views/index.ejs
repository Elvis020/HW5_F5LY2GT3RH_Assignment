<body>
    <div class="outer-guard">
        <% devices.forEach(function(device){ %>
            <div class="spacey-card">
                <h4>Countries: <%= device.Combined_Key %> 😻  </h4>
                <ul>
                    <li>Active Cases ⚔ : <%= device.Active %></li> 
                    <li>Recoveries 🏥 : <%= device.Recovered %></li> 
                    <li>No. of Deaths 🏴‍☠️ : <%= device.Deaths %></li> 
                </ul>
            </div>
        <% }); %>
        </div>

